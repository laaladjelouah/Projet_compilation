package expressions;

import semantic.EnumTag;

public class IDExpression extends Expression {
	
	private String s;
	
	public IDExpression(String id) {
		super(null, null, EnumTag.IDENTIFIER);
		s = id;
	}
	
	public String getNameVar() {
		return s;
	}

	

	
	public String toString() {
		return "ID : " + s + " ";
	}
	

}
